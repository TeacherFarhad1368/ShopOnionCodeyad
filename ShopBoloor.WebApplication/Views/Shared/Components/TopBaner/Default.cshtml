@model List<Site.Application.Contract.BanerApplication.Query.BanerForUiQueryModel>

@if(Model.Count() > 0)
{
    var baner = Model.First();
    <div class="banner-ads d-md-block d-none">
        <section class="banner">
            <a href="@baner.Url">
                <img src="@baner.ImageName" alt="@baner.ImageAlt">
            </a>
        </section>
    </div>
}