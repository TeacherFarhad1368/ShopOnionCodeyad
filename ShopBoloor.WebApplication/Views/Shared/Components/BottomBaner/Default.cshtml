@model List<Site.Application.Contract.BanerApplication.Query.BanerForUiQueryModel>

@if(Model.Count() > 0)
{
    var baner = Model.First();
    <div class="row banner-ads">
        <div class="col-12">
            <div class="row">
                <div class="col-12">
                    <div class="widget widget-banner card">
                        <a href="@baner.Url" target="_blank">
                            <img class="img-fluid" src="@baner.ImageName" alt="@baner.ImageAlt">
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
}