@model List<Query.Contract.UI.Product.GalleryForProductSingleQueryModel>
@using Shared.Application;
@if(Model.Count() > 0)
{
    foreach (var item in Model)
    {
        <li class="item">
            <a href="tester" class="elevatezoom-gallery"
               data-image="@FileDirectories.ProductGalleryImageDirectory500@item.ImageName"
               data-zoom-image="@FileDirectories.ProductGalleryImageDirectory@item.ImageName">
                <img src="@FileDirectories.ProductGalleryImageDirectory100@item.ImageName" width="100" />
            </a>
        </li>
    }
}