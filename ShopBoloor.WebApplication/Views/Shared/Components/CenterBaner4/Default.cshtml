@model List<Site.Application.Contract.BanerApplication.Query.BanerForUiQueryModel>

@if(Model.Count() > 0)
{
    <div class="row banner-ads">
        <div class="col-12">
            <div class="row">
                @foreach(var item in Model)
                {
                    <div class="col-6 col-lg-3">
                        <div class="widget-banner card">
                            <a href="@item.Url" target="_blank">
                                <img class="img-fluid" src="@item.ImageName" alt="@item.ImageAlt">
                            </a>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
}